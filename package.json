{
  "name": "polyglot-system",
  "version": "1.0.0",
  "description": "Advanced multi-language system with Node.js, C++, Python, Rust, Go, and WebAssembly integration",
  "main": "src/index.js",
  "scripts": {
    "build": "npm run build:cpp && npm run build:rust && npm run build:go && npm run build:wasm",
    "build:cpp": "node-gyp rebuild",
    "build:rust": "cd rust-module && cargo build --release",
    "build:go": "cd go-module && go build -buildmode=c-shared -o ../build/go-module.so main.go",
    "build:wasm": "cd wasm-module && emcc -O3 -s WASM=1 -s EXPORTED_FUNCTIONS='[\"_fibonacci\", \"_prime_check\"]' -o ../build/math.js math.c",
    "start": "node src/index.js",
    "dev": "nodemon src/index.js",
    "test": "jest",
    "benchmark": "node benchmarks/performance.js",
    "docker:build": "docker build -t polyglot-system .",
    "docker:run": "docker run -p 3000:3000 polyglot-system"
  },
  "dependencies": {
    "express": "^4.18.2",
    "ws": "^8.14.2",
    "sqlite3": "^5.1.6",
    "redis": "^4.6.10",
    "mongoose": "^7.6.3",
    "bcrypt": "^5.1.1",
    "jsonwebtoken": "^9.0.2",
    "helmet": "^7.1.0",
    "cors": "^2.8.5",
    "compression": "^1.7.4",
    "winston": "^3.11.0",
    "joi": "^17.11.0",
    "dotenv": "^16.3.1",
    "node-cron": "^3.0.2",
    "sharp": "^0.32.6",
    "puppeteer": "^21.4.1",
    "cheerio": "^1.0.0-rc.12",
    "axios": "^1.5.1",
    "socket.io": "^4.7.4",
    "graphql": "^16.8.1",
    "apollo-server-express": "^3.12.1",
    "grpc": "^1.24.11",
    "@grpc/grpc-js": "^1.9.7",
    "protobufjs": "^7.2.5",
    "kafka-node": "^5.0.0",
    "amqplib": "^0.10.3",
    "ioredis": "^5.3.2",
    "pg": "^8.11.3",
    "mysql2": "^3.6.3",
    "elasticsearch": "^16.7.3",
    "neo4j-driver": "^5.13.0",
    "influxdb-client": "^1.33.2"
  },
  "devDependencies": {
    "nodemon": "^3.0.1",
    "jest": "^29.7.0",
    "supertest": "^6.3.3",
    "eslint": "^8.52.0",
    "prettier": "^3.0.3",
    "node-gyp": "^9.4.0",
    "@types/node": "^20.8.7",
    "typescript": "^5.2.2",
    "ts-node": "^10.9.1"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  },
  "keywords": [
    "nodejs",
    "cpp",
    "python",
    "rust",
    "go",
    "webassembly",
    "microservices",
    "performance",
    "polyglot"
  ],
  "author": "Polyglot System Team",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/polyglot-system/polyglot-system.git"
  }
}